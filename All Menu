    VirtualUser:CaptureController()
    VirtualUser:ClickButton2(Vector2.new())
end)

-- Notifikasi saat money berhasil diubah (optional)
local function showMoneyNotification(amount)
    local notif = Instance.new("TextLabel")
    notif.Size = UDim2.new(0, 200, 0, 40)
    notif.Position = UDim2.new(0.5, -100, 0, 100)
    notif.BackgroundColor3 = Color3.fromRGB(0, 200, 100)
    notif.BackgroundTransparency = 0.3
    notif.TextColor3 = Color3.fromRGB(255, 255, 255)
    notif.Text = "ğŸ’° +" .. formatNumber(amount)
    notif.Font = Enum.Font.GothamBold
    notif.TextSize = 16
    notif.Parent = ScreenGui
    
    local notifCorner = Instance.new("UICorner")
    notifCorner.CornerRadius = UDim.new(0, 10)
    notifCorner.Parent = notif
    
    -- Fade out animation
    task.wait(1)
    local tweenInfo = TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
    local tween = TweenService:Create(notif, tweenInfo, {
        BackgroundTransparency = 1,
        TextTransparency = 1,
        Position = UDim2.new(0.5, -100, 0, 50)
    })
    tween:Play()
    
    tween.Completed:Connect(function()
        notif:Destroy()
    end)
end

-- Auto-detect money system saat character spawn
LocalPlayer.CharacterAdded:Connect(function()
    task.wait(2) -- Tunggu game load
    
    if infMoneyEnabled then
        local currentMoney = getCurrentMoney()
        if currentMoney > 0 then
            print("ğŸ’° Money system detected! Current:", formatNumber(currentMoney))
            StatusLabel.Text = "Status: Money System Detected âœ…"
            StatusLabel.TextColor3 = Color3.fromRGB(0, 255, 100)
        else
            warn("ğŸ’° Money system not detected yet...")
            StatusLabel.Text = "Status: Searching for money system..."
            StatusLabel.TextColor3 = Color3.fromRGB(255, 200, 100)
        end
    end
end)

